{
	"name": "greenpeace/planet4-test-venus",
	"description": "Greenpeace P4 test-venus",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-master-theme" : "dev-planet-4833",
		"greenpeace/planet4-styleguide" : "dev-planet-4833",
		"greenpeace/planet4-plugin-gutenberg-blocks" : "dev-master",
		"greenpeace/planet4-plugin-gutenberg-engagingnetworks" : "dev-master"
	},
	"data_from": "international",
	"scripts": {
		"install:plugin-wpml-string-translation": "wp plugin install --activate https://raw.githubusercontent.com/greenpeace/planet4-3rdparty-plugins/master/wpml-string-translation.2.9.2.zip",
		"install:plugin-searchwp-wpml": "wp plugin install --activate https://raw.githubusercontent.com/greenpeace/planet4-3rdparty-plugins/master/searchwp-wpml-1.4.0.zip",
		"install:plugin-sitepress-multilingual-cms": "wp plugin install --activate https://raw.githubusercontent.com/greenpeace/planet4-3rdparty-plugins/master/sitepress-multilingual-cms.4.1.3.zip",
		"install:plugin-wpml-translation-management": "wp plugin install --activate https://raw.githubusercontent.com/greenpeace/planet4-3rdparty-plugins/master/wpml-translation-management.2.7.2.zip",
		"install:language-fr_CA": "wp language core install fr_CA",
		"site:custom": [
			"@install:plugin-wpml-translation-management",
			"@install:plugin-sitepress-multilingual-cms",
			"@install:plugin-wpml-string-translation",
			"@install:plugin-searchwp-wpml",
			"@install:language-fr_CA"
		]
	}
}
